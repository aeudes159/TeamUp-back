# TeamUp Backend Environment Variables
# Copy this file to .env and fill in your values

# PostgreSQL JDBC connection URL
# Examples:
#   Local PostgreSQL:      jdbc:postgresql://localhost:5432/teamup
#   Local Supabase:        jdbc:postgresql://localhost:54322/postgres
#   Remote Supabase:       jdbc:postgresql://db.xxxxx.supabase.co:5432/postgres
SPRING_DATASOURCE_URL=jdbc:postgresql://localhost:54322/postgres

# Database username
SPRING_DATASOURCE_USERNAME=postgres

# Database password
SPRING_DATASOURCE_PASSWORD=postgres

# Server port (default: 8080)
PORT=8080
